{
    "name": "kanso-spine-todo",
    "version": "0.0.1",
    "description": "A very simple Kanso - Spine Todo List app",
    "dependencies": {
        "attachments": null,
        "modules": null,
        "eco": null,
        "spine": null,
        "jquery": null,
        "json2": null,
        "es5shim": null,
        "stylus-precompiler": null,
        "coffee-script-precompiler": null,
        "spine-adapter": null
    },
    "load": "server/app",
    "attachments": ["index.html"],
    "stylus": {
        "compile": ["css/application.styl"]
    },
    "eco": {
        "templates": ["views"]
    },
    "coffee-script": {
        "modules": ["App.coffee", "lib", "controllers", "models", "server"]
    }
}